<?xml version="1.0"?>
<launch>
  <param name="/use_sim_time" value="true" />

  <!-- start world -->
  <node name="gazebo" pkg="gazebo_ros" type="gazebo" 
   args="$(find raspimouse_gazebo)/worlds/traffic_regulation.world" respawn="false" output="screen" />
  
  <!-- start gui -->
  <!-- <node name="gazebo_gui" pkg="gazebo" type="gui" respawn="false" output="screen"/> -->

  <!-- include our robots -->
  <include file="$(find raspimouse_gazebo)/launch/robotsmulticolor.launch"/>
</launch>